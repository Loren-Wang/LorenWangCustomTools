<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvlwLeftRightIconShowEditText">
        <!--左侧icon-->
        <attr name="avlw_lrice_leftIconRes" format="reference" />
        <!--左侧icon宽-->
        <attr name="avlw_lrice_leftIconWidth" format="dimension" />
        <!--左侧icon高-->
        <attr name="avlw_lrice_leftIconHeight" format="dimension" />
        <!--右侧icon-->
        <attr name="avlw_lrice_rightIconRes" format="reference" />
        <!--右侧icon宽-->
        <attr name="avlw_lrice_rightIconWidth" format="dimension" />
        <!--右侧icon高-->
        <attr name="avlw_lrice_rightIconHeight" format="dimension" />
        <!--右侧图标点击是否是清除文本-->
        <attr name="avlw_lrice_rightIconClickClearText" format="boolean" />
    </declare-styleable>
</resources>
