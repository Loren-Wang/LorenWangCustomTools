<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvlwTitleBarHeadView">
        <!--标题样式-->
        <attr name="avlwTitleTextSize" format="dimension" />
        <attr name="avlwTitleTextColor" format="color" />
        <attr name="avlwTitleText" format="string" />
        <!--左侧按钮时样式-->
        <attr name="avlwLeftBtnTextSize" format="dimension" />
        <attr name="avlwLeftBtnTextColor" format="color" />
        <attr name="avlwLeftBtnText" format="string" />
        <!--右侧按钮时样式-->
        <attr name="avlwRightBtnTextSize" format="dimension" />
        <attr name="avlwRightBtnTextColor" format="color" />
        <attr name="avlwRightBtnText" format="string" />
        <!--自定义布局-->
        <attr name="customLayout" format="reference" />
        <!--背景颜色-->
        <attr name="viewBgColor" format="color" />
        <!--左侧样式,宽高使用一致的-->
        <attr name="avlwLeftWidth" format="dimension" />
        <attr name="avlwLeftHeight" format="dimension" />
        <attr name="avlwLeftBgColor" format="color" />
        <attr name="avlwLeftBgRes" format="reference" />
        <!--左侧边距-->
        <attr name="avlwLeftDistanceLeft" format="dimension" />
        <attr name="avlwLeftDistanceTop" format="dimension" />
        <attr name="bavlwLeftDistanceRight" format="dimension" />
        <attr name="avlwLeftDistanceBottom" format="dimension" />
        <!--右侧侧样式,宽高使用一致的-->
        <attr name="avlwRightWidth" format="dimension" />
        <attr name="avlwRightHeight" format="dimension" />
        <attr name="avlwRightBgColor" format="color" />
        <attr name="avlwRightBgRes" format="reference" />
        <!--右侧边距-->
        <attr name="avlwRightDistanceLeft" format="dimension" />
        <attr name="avlwRightDistanceTop" format="dimension" />
        <attr name="avlwRightDistanceRight" format="dimension" />
        <attr name="avlwRightDistanceBottom" format="dimension" />
        <!--布局类型-->
        <attr name="avlwLayoutType" format="enum">
            <enum name="LAYOUT_TYPE_0" value="0" />//（0）自定义view
            <enum name="LAYOUT_TYPE_1" value="1" />//（1）后退按钮图片、标题
            <enum name="LAYOUT_TYPE_2" value="2" />//（2）后退按钮图片、标题、右侧按钮
            <enum name="LAYOUT_TYPE_3" value="3" />//（3）标题、右侧按钮
            <enum name="LAYOUT_TYPE_4" value="4" />//（4）左侧按钮、标题
            <enum name="LAYOUT_TYPE_5" value="5" />//（5）左侧按钮、右侧按钮
            <enum name="LAYOUT_TYPE_6" value="6" />//（6）后退按钮图片、右侧按钮
            <enum name="LAYOUT_TYPE_7" value="7" />//（7）后退按钮图片、标题、右侧图标
            <enum name="LAYOUT_TYPE_8" value="8" />//（8）标题、右侧图标
            <enum name="LAYOUT_TYPE_9" value="9" />//（9）后退按钮图片、右侧图标
        </attr>
    </declare-styleable>
</resources>
