<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvlwTitleBarHeadView">
        <!--标题样式-->
        <attr name="avlw_tbh_titleTextSize" format="dimension" />
        <attr name="avlw_tbh_titleTextColor" format="color" />
        <attr name="avlw_tbh_titleText" format="string" />
        <!--左侧按钮时样式-->
        <attr name="avlw_tbh_leftBtnTextSize" format="dimension" />
        <attr name="avlw_tbh_leftBtnTextColor" format="color" />
        <attr name="avlw_tbh_leftBtnText" format="string" />
        <!--右侧按钮时样式-->
        <attr name="avlw_tbh_rightBtnTextSize" format="dimension" />
        <attr name="avlw_tbh_rightBtnTextColor" format="color" />
        <attr name="avlw_tbh_rightBtnText" format="string" />
        <!--自定义布局-->
        <attr name="avlw_tbh_customLayout" format="reference" />
        <!--背景颜色-->
        <attr name="avlw_tbh_viewBgColor" format="color" />
        <!--左侧样式,宽高使用一致的-->
        <attr name="avlw_tbh_leftWidth" format="dimension" />
        <attr name="avlw_tbh_leftHeight" format="dimension" />
        <attr name="avlw_tbh_leftBgColor" format="color" />
        <attr name="avlw_tbh_leftBgRes" format="reference" />
        <!--左侧边距-->
        <attr name="avlw_tbh_leftDistanceLeft" format="dimension" />
        <attr name="avlw_tbh_leftDistanceTop" format="dimension" />
        <attr name="avlw_tbh_leftDistanceRight" format="dimension" />
        <attr name="avlw_tbh_leftDistanceBottom" format="dimension" />
        <!--右侧侧样式,宽高使用一致的-->
        <attr name="avlw_tbh_rightWidth" format="dimension" />
        <attr name="avlw_tbh_rightHeight" format="dimension" />
        <attr name="avlw_tbh_rightBgColor" format="color" />
        <attr name="avlw_tbh_rightBgRes" format="reference" />
        <!--右侧边距-->
        <attr name="avlw_tbh_rightDistanceLeft" format="dimension" />
        <attr name="avlw_tbh_rightDistanceTop" format="dimension" />
        <attr name="avlw_tbh_rightDistanceRight" format="dimension" />
        <attr name="avlw_tbh_rightDistanceBottom" format="dimension" />
        <!--布局类型-->
        <attr name="avlw_tbh_layoutType" format="enum">
            <enum name="LAYOUT_TYPE_0" value="0" />//（0）自定义view
            <enum name="LAYOUT_TYPE_1" value="1" />//（1）后退按钮图片、标题
            <enum name="LAYOUT_TYPE_2" value="2" />//（2）后退按钮图片、标题、右侧按钮
            <enum name="LAYOUT_TYPE_3" value="3" />//（3）标题、右侧按钮
            <enum name="LAYOUT_TYPE_4" value="4" />//（4）左侧按钮、标题
            <enum name="LAYOUT_TYPE_5" value="5" />//（5）左侧按钮、右侧按钮
            <enum name="LAYOUT_TYPE_6" value="6" />//（6）后退按钮图片、右侧按钮
            <enum name="LAYOUT_TYPE_7" value="7" />//（7）后退按钮图片、标题、右侧图标
            <enum name="LAYOUT_TYPE_8" value="8" />//（8）标题、右侧图标
            <enum name="LAYOUT_TYPE_9" value="9" />//（9）后退按钮图片、右侧图标
        </attr>
    </declare-styleable>
</resources>
