<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--文本选择列表radiobutton样式-->
    <style name="AvlwRadioButtonTextListSelect">
        <item name="android:paddingStart">@dimen/avlw_default_content_distance</item>
        <item name="android:paddingEnd">@dimen/avlw_default_content_distance</item>
        <item name="android:textSize">@dimen/textSize14</item>
        <item name="android:textColor">#464646</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:button">@null</item>
        <item name="android:drawableRight">@android:drawable/btn_radio</item>
    </style>

    <!--默认分隔线样式-->
    <style name="AvlwDefaultLineBetween">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#dcdcdc</item>
    </style>
</resources>
