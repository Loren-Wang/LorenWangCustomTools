<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AvlwWheelRecyclerView">
        <!--item高度-->
        <attr name="avlw_wr_itemHeight" format="dimension" />
        <!--选中文本大小-->
        <attr name="avlw_wr_selectTextSize" format="dimension" />
        <!--未选中文本大小-->
        <attr name="avlw_wr_unSelectTextSize" format="dimension" />
        <!--选中文本颜色-->
        <attr name="avlw_wr_selectTextColor" format="color" />
        <!--未选中文本颜色-->
        <attr name="avlw_wr_unSelectTextColor" format="color" />
        <!--选中文本是否加粗-->
        <attr name="avlw_wr_selectTextIsBold" format="boolean" />
        <!--未选中文本是否加粗-->
        <attr name="avlw_wr_unSelectTextIsBold" format="boolean" />
        <!--单侧显示数量-->
        <attr name="avlw_wr_wheelOffset" format="integer" />
        <!--分隔线宽度-->
        <attr name="avlw_wr_dividerWidth" format="dimension" />
        <!--分隔线高度-->
        <attr name="avlw_wr_dividerHeight" format="dimension" />
        <!--分隔线颜色-->
        <attr name="avlw_wr_dividerColor" format="color" />
    </declare-styleable>

</resources>
