<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvlwKerleyOrDottedViewAvlw">
        <!--宽度-->
        <attr name="avlw_kv_width" format="dimension" />
        <!--高度-->
        <attr name="avlw_kv_height" format="dimension" />
        <!--直径-->
        <attr name="avlw_kv_radius" format="dimension" />
        <!--间距-->
        <attr name="avlw_kv_distance" format="dimension" />
        <!--颜色-->
        <attr name="avlw_kv_color" format="color" />
        <!--方向-->
        <attr name="avlw_kv_orientation">
            <!-- Defines an horizontal widget. -->
            <enum name="horizontal" value="0" />
            <!-- Defines a vertical widget. -->
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>
