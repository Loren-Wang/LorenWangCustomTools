<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--后退按钮或者图片样式-->
    <style name="AvlwTitleBarHeadViewLeft">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:src">@mipmap/avlw_icon_arrow_left</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <!--标题文本样式-->
    <style name="AvlwTitleBarHeadViewTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
    </style>
    <!--标题文本样式-->
    <style name="AvlwTitleBarHeadViewRight" parent="AvlwTitleBarHeadViewLeft">
        <item name="android:background">@null</item>
        <item name="android:src">@mipmap/avlw_icon_arrow_left</item>
        <item name="layout_constraintLeft_toLeftOf">@null</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
    </style>
</resources>
