<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvlwCalendarView">
        <!--周的第一天显示是周几-->
        <attr name="avlw_caledar_weekFirst">
            <enum name="sunday" value="0" />
            <enum name="monday" value="1" />
            <enum name="tuesday" value="2" />
            <enum name="wednesday" value="3" />
            <enum name="thursday" value="4" />
            <enum name="friday" value="5" />
            <enum name="saturday" value="6" />
        </attr>
        <!--是否只显示单月-->
        <attr name="avlw_caledar_showOnlyMonth" format="boolean" />
        <!--是否是单选-->
        <attr name="avlw_caledar_selecOne" format="boolean" />
        <!--是否使用自动高度切换-->
        <attr name="avlw_caledar_useAutoHeight" format="boolean" />
    </declare-styleable>
</resources>
