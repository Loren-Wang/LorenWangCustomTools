<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomDrawableButton">
        <attr name="drawablePosi">
            <enum name="DRAWABLE_POSI_NONE" value="0"/>
            <enum name="DRAWABLE_POSI_LEFT" value="1"/>
            <enum name="DRAWABLE_POSI_TOP" value="2"/>
            <enum name="DRAWABLE_POSI_RIGHT" value="3"/>
            <enum name="DRAWABLE_POSI_BOTTOM" value="4"/>
        </attr>
        <attr name="drawableWidth" format="dimension"/>
        <attr name="drawableHeight" format="dimension"/>
        <attr name="drawableTextDistance" format="dimension"/>
        <attr name="drawableResId" format="reference"/>
    </declare-styleable>

</resources>
