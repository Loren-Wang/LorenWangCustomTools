<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.ProgressActivity"
    android:padding="20dp">

    <android.lorenwang.customview.progress.AvlwProgressBar
        android:layout_width="match_parent"
        android:layout_height="10dp"
        app:avlwProgressBigHeight="10dp"
        app:avlwProgressBgImage="@drawable/sidebar_select"
        app:avlwProgressCacheImage="@drawable/progressbar_cache"
        app:avlwProgressImage="@drawable/progressbar_progress"
        app:avlwProgressCurrentImage="@drawable/progressbar_current"
        app:avlwProgressCurrentHeight="20dp"
        app:avlwProgressCurrentWidth="20dp"
        app:avlwProgress="0.2"
        app:avlwProgressCache="0.5"
        tools:ignore="MissingPrefix" />

    <android.lorenwang.customview.progress.AvlwProgressBar
        android:layout_width="match_parent"
        android:layout_height="10dp"
        app:avlwProgressBigHeight="10dp"
        app:avlwProgressBgImage="@drawable/sidebar_select"
        app:avlwProgressCacheImage="@drawable/progressbar_cache"
        app:avlwProgressImage="@drawable/progressbar_progress"
        app:avlwProgressCurrentImage="@drawable/progressbar_current"
        app:avlwProgressCurrentHeight="10dp"
        app:avlwProgressCurrentWidth="10dp"
        android:layout_marginTop="10dp"
        app:avlwProgress="0.2"
        app:avlwProgressCache="0.5"
        tools:ignore="MissingPrefix" />


</androidx.appcompat.widget.LinearLayoutCompat>
