<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--默认标题栏样式-->
    <style name="DefaultTitleBarHeadView">
        <item name="avlwLeftBgRes">@android:drawable/ic_notification_clear_all</item>
        <item name="avlwLeftDistanceLeft">16dp</item>
        <item name="bavlwLeftDistanceRight">16dp</item>
        <item name="avlwTitleTextSize">@dimen/textSize16</item>
        <item name="avlwTitleTextColor">#464646</item>
    </style>

</resources>
